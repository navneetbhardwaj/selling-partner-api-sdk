{
    "name": "amzn-spapi/sdk",
    "description": "Amazon Selling Partner APIs official client library for PHP.",
    "type": "library",
    "keywords": [
        "sp-api",
        "amazon",
        "sdk",
        "openapi-generator",
        "php",
        "rest"
    ],
    "homepage": "https://github.com/amzn/selling-partner-api-sdk",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Amazon API Services",
            "homepage": "https://developer-docs.amazon.com/sp-api"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.0",
        "aws/aws-sdk-php": "^3.228",
        "symfony/rate-limiter": "^7.2",
        "vlucas/phpdotenv": "^5.6",
        "ext-openssl": "*",
        "dallgoot/yaml": "^1.0"
    },
    "conflict": {
        "guzzlehttp/guzzle": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "autoload": {
        "psr-4": {
            "SpApi\\AuthAndAuth\\": "src/authandauth",
            "SpApi\\": "lib/",
            "SpApi\\Test\\" : "test/"
        }
    }
}
